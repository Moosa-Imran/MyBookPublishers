    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords"
        content="Educational Books, e-Learning, Kids Learning, Educational Resources, Online Education">
    <meta name="description"
        content="Your Gateway to Educational Excellence. My Book Publishers offers smarter books for smarter minds, with digital learning resources and interactive content for kids and students.">
    <meta name="copyright" content="My Book Publishers">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Title -->
    <title>My Book Publishers - Your Gateway to Educational Excellence</title>

    <!-- Favicon -->
